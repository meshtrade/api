{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "lib": ["dom", "dom.iterable", "es6"],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "es5",
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules/", "build/", ".docusaurus/"]
}
