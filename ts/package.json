{
  "name": "@meshtrade/api",
  "version": "1.21.0",
  "description": "Mesh Typescript SDK",
  "author": "Bernard Bussy <bernard@meshtrade.co>",
  "keywords": [
    "api",
    "meshtrade",
    "sdk"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/meshtrade/api.git"
  },
  "homepage": "https://meshtrade.github.io/api",
  "type": "commonjs",
  "exports": {
